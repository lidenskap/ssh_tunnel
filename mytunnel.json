{ "id": "mytunnel",
    "ssh_hostname": "example.com",
    "ssh_port": 22,
    "ssh_user": "user",
    "ssh_private_key": "/path/to/.ssh/id_rsa",
    "ssh_public_key": "/path/to/.ssh/id_rsa.pub",
    "connect_port": 1234,
    "forward_host": "remotehost.com",
    "forward_port": 80,
    "tunnel_direction": "FWD",
    "ssh_cmd": "/usr/bin/autossh",
    "skip_hostkey_auth": false,
    "ssh_known_hosts": "/path/to/known_hosts",
    "action": "add" }
